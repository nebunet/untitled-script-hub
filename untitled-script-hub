local function OGVTH_fake_script() -- Sections.ScriptHandler 
	local script = Instance.new('LocalScript', Sections)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local sections = script.Parent
	local main = sections.Parent
	local modules = main.Modules
	local SL = require(modules.ScriptLoader)
	
	for _, button in pairs(sections.Frames:GetDescendants()) do
		
		if button:IsA("TextButton") or button:IsA("ImageButton") then
			
			button.Activated:Connect(function()
				
				local executedScript = SL.ReturnNeededScript(button.Name)
				
				if executedScript ~= false then
					
					if button:FindFirstChild("NetlessNotNeeded") then	
						SL.LoadScript(executedScript, false)			
					else
						SL.LoadScript(executedScript, true)
					end
					
				end
				
			end)
			
		end
		
	end
end
coroutine.wrap(OGVTH_fake_script)()
local function ZXFDQCH_fake_script() -- Main.Hide 
	local script = Instance.new('LocalScript', Main)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local main = script.Parent
	local startup = main.Parent.Opening
	local sideBar = main.SideBar
	
	main.Visible = false
	startup.Visible = true
	main.Size = UDim2.new(0, 300, 0, 0)
	sideBar.Size = UDim2.new(0, 33, 0, 0)
	
	for i,v in pairs(main:GetDescendants()) do
		
		if v:IsA("TextLabel") then
			v.TextTransparency = 1
		elseif v:IsA("ImageLabel") or v:IsA("ImageButton") then
			v.ImageTransparency = 1
		end
		
	end
end
coroutine.wrap(ZXFDQCH_fake_script)()
